#include<bits/stdc++.h>
#include<queue>
#include<stack>
using namespace std;
stack <int> st;
queue <int> que;
int main(){
    int no,fg;
    cin>>no;
    int arr[no];
    for(int i=0;i<no;i++){
        cin>>arr[i];
        fg=0;
        for(int j=2;j<=arr[i]/2;j++){
            if(arr[i]%j==0){
                st.push(arr[i]);
                fg=1;
                break;
            }
        }
        if(fg==0){
            que.push(arr[i]);
        }
    }
    while(!que.empty()){
        cout<<que.front()<<" ";
        que.pop();
    }
    cout<<"\n";
    while(!st.empty()){
        cout<<st.top()<<" ";
        st.pop();
    }
}
